		return;
	}

	if( newValue == ''){
		g_form.clearValue('delivered_by_employee_number');
		return;
	}
	
	var gr = new GlideRecord('sys_user');
	gr.addQuery('sys_id',newValue);
	gr.query(function (gr){

		if(gr.next()){

			g_form.setValue('delivered_by_employee_number',gr.getValue('employee_number'));
		}
	});
}]]></script>
