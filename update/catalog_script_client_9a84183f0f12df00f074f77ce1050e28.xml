		return;
	}

	if( newValue == ''){
		g_form.clearValue('received_by_employee_id');
		return;
	}
	
	var gr = new GlideRecord('sys_user');
	gr.addQuery('sys_id',newValue);
	gr.query(function (gr){

		if(gr.next()){

			g_form.setValue('received_by_employee_id',gr.getValue('employee_number'));
		}
	});
}]]></script>
