        <name>Ensure pistol count is valid</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

   var pattern =/^[0-9]*$/;

	if (pattern.test(newValue) == false) {
		g_form.setValue('pistol_count', '');
		alert("Kun numeriske værdier er tilladt - only numeric values are allowed");
		return;
	} else {
		var intValue = parseInt(newValue);
		if(intValue > 10) {
			alert("Verficer at det indtastet antal til pistoler stemmers overens.");
		}
	}


}]]></script>
